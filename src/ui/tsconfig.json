{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "outDir": "../../dist/public/build",
    "resolveJsonModule": true,
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "useUnknownInCatchVariables": false,
    "paths": {
      "components/*": ["./components/*"],
      "constants/*": ["./constants/*"],
      "datalens": ["./datalens/index.tsx"],
      "datalens/*": ["./datalens/*"],
      "hooks": ["./hooks/index.ts"],
      "hooks/*": ["./hooks/*"],
      "hoc": ["./hoc"],
      "hoc/*": ["./hoc/*"],
      "i18n": ["../i18n/index.ts"],
      "i18n/*": ["../i18n/*"],
      "index": ["./index.ts"],
      "libs": ["./libs/index.ts"],
      "libs/*": ["./libs/*"],
      "shared": ["../shared/index.ts"],
      "shared/*": ["../shared/*"],
      "store": ["./store/index.ts"],
      "store/*": ["./store/*"],
      "typings": ["./typings/index.ts"],
      "typings/*": ["./typings/*"],
      "ui": ["./index.ts"],
      "ui/*": ["./*"],
      "utils": ["./utils/index.ts"],
      "utils/*": ["./utils/*"],
      "units/*": ["./units/*"]
    }
  },
  "include": [
    "**/*",
    "../@types",
    "../i18n",
    "../shared/**/*",
    "../../node_modules/@gravity-ui/chartkit/build/plugins/highcharts/types/highcharts-extends.d.ts",
    "../../../../node_modules/@gravity-ui/chartkit/build/plugins/highcharts/types/highcharts-extends.d.ts"
  ]
}
